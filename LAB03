mov ah,1h
INT 21H
mov dl,103
mov ah,1h
INT 21h
mov dl,105
mov ah,2h
INT 21H
mov dl, 101-32
mov ah,2h
INT 21H
mov dl,102-32
mov ah,2h
INT 21H
mov dl,103-32
mov ah,2h
INT 21H
mov dl,105
mov ah,2h
INT 21H
main endp
end main
