Task 02:
;to display the character'a' org 100H
.data
.code
main proc
mov dl,'a' mov ah,2h
INT 21h
mov dl,'n' mov ah,2h
INT 21h
mov dl,'f' mov ah,2h
INT 21h
mov dl,'a' mov ah,2h
INT 21h
mov dl,'l' mov ah,2h
INT 21h
main endp
end main
Task 03:
org 100H
.data
.code
main proc
mov dl,101
mov ah,1h
INT 21H
